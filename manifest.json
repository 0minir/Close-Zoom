{
	"name": "Close Zoom",
	"description": "Closes Zoom tabs after 10 seconds.",
	"version": "1.0",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
   "permissions": [ "activeTab" ],
   "host_permissions": [ "https://*.zoom.us/*" ]
}
